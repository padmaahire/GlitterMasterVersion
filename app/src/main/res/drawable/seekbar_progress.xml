<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:id="@+id/progressshape" >
        <clip>
            <shape
                android:shape="rectangle" >
                <size android:height="0.1dp"/>
                <corners
                    android:radius="8dp" />
               <gradient
                android:startColor="#D48A00"
                android:centerColor="#CE8800"
                android:endColor="#F1AE00"
                android:angle="270"/>
            </shape>
        </clip>
    </item>
</layer-list>